import os
import requests

from flask import Flask, render_template, requests

app = Flask(__name__)


@app.route("/", methods = ["GET", "POST"])
def hello_world():
    return "This is a case study for put.io"
    


if __name__ == "__main__":
    app.run(debug=True, host="0.0.0.0", port=int(os.environ.get("PORT", 8080)))
